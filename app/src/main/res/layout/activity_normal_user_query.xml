<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".NormalUserQuery">

    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/ApellidoTextView"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="206dp" android:layout_marginStart="206dp"
            app:layout_constraintTop_toBottomOf="@+id/NombreTextView" android:layout_marginTop="16dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="205dp"
            android:layout_marginRight="205dp"/>
    <TextView
            android:text="@string/informacionUsuario"
            android:layout_width="0dp"
            android:layout_height="wrap_content" android:id="@+id/textView8"
            android:textStyle="bold"
            android:layout_marginTop="16dp"
            app:layout_constraintTop_toBottomOf="@+id/BotonConsultarUsuario"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="130dp"
            android:layout_marginStart="130dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="131dp" android:layout_marginRight="131dp"/>
    <EditText
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:inputType="number"
            android:ems="10"
            android:id="@+id/DocConsultaEditText"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="16dp" android:layout_marginRight="16dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="16dp"
            android:layout_marginStart="16dp" android:layout_marginTop="29dp"
            app:layout_constraintTop_toBottomOf="@+id/textView7"/>
    <Button
            android:text="@string/consultarUsuario"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/BotonConsultarUsuario"
            android:background="@android:color/holo_blue_dark" android:textColor="#FFFFFF"
            android:layout_marginTop="32dp"
            app:layout_constraintTop_toBottomOf="@+id/DocConsultaEditText"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="134dp"
            android:layout_marginStart="134dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="133dp" android:layout_marginRight="133dp" android:paddingLeft="5dp"
            android:paddingRight="5dp"/>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/NombreTextView"
            app:layout_constraintTop_toBottomOf="@+id/textView8"
            android:layout_marginTop="16dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="206dp" android:layout_marginStart="206dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="205dp"
            android:layout_marginRight="205dp"/>
    <TextView
            android:text="@string/ingreseDocumento"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/textView7"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="64dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="16dp" android:layout_marginStart="16dp"
            app:layout_constraintEnd_toEndOf="parent"/>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/CorreoTextView"
            app:layout_constraintTop_toBottomOf="@+id/ApellidoTextView"
            android:layout_marginTop="16dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="206dp" android:layout_marginStart="206dp"
            android:layout_marginEnd="205dp" android:layout_marginRight="205dp"
            app:layout_constraintEnd_toEndOf="parent"/>
    <Button
            android:text="@string/irGestionColas"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/IrAdminQuery"
            app:layout_constraintTop_toBottomOf="@+id/CorreoTextView"
            android:layout_marginTop="32dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="161dp" android:layout_marginStart="161dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="162dp"
            android:layout_marginRight="162dp" android:background="@android:color/holo_blue_dark"
            android:textColor="#FFFFFF" android:paddingLeft="5dp" android:paddingRight="5dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>